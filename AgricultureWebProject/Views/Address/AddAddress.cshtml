@model EntityLayer.Concrete.Address

@{
    ViewData["Title"] = "AddAddress";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h4 class="card-title">Yeni Adres Ekleme Sayfası</h4>
			<form class="forms-sample" method="post">
				<div class="form-group">
					<label for="exampleInputName1">Açıklama-1</label>
					<span class="text-danger" asp-validation-for="Descripiton1"></span>
					<input type="text" class="form-control" id="exampleInputName1" placeholder="Başlık" asp-for="Descripiton1">
				</div>
				<div class="form-group">
					<label for="exampleInputName1">Açıklama-2</label>
					<span class="text-danger" asp-validation-for="Descripiton2"></span>
					<input type="text" class="form-control" id="exampleInputName1" placeholder="Başlık" asp-for="Descripiton2">
				</div>
				<div class="form-group">
					<label for="exampleInputName1">Açıklama-3</label>
					<span class="text-danger" asp-validation-for="Descripiton3"></span>
					<input type="text" class="form-control" id="exampleInputName1" placeholder="Başlık" asp-for="Descripiton3">
				</div>
				<div class="form-group">
					<label for="exampleInputName1">Açıklama-4</label>
					<span class="text-danger" asp-validation-for="Descripiton4"></span>
					<input type="text" class="form-control" id="exampleInputName1" placeholder="Başlık" asp-for="Descripiton4">
				</div>
				<div class="form-group">
					<label for="exampleInputName1">Açıklama-5</label>
					<span class="text-danger" asp-validation-for="Mapinfo"></span>
					<input type="text" class="form-control" id="exampleInputName1" placeholder="Başlık" asp-for="Mapinfo">
				</div>
				<button type="submit" class="btn btn-dark">Kaydet</button>
				<button type="reset" class="btn btn-outline-warning">Vazgeç</button>
			</form>
		</div>
	</div>
</div>

